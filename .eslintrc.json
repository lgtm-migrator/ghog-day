{
    "env": {
      "es6": true,
      "node": true
    },
    "extends": ["eslint:recommended"],
    "parserOptions": {
      "ecmaVersion": 9,
      "sourceType": "module"
    },
    "rules": {
      "indent": ["error", 2, { "SwitchCase": 1, "ignoredNodes": ["TemplateLiteral *"] }],
      "linebreak-style": 0,
      "quotes": ["error", "single"],
      "comma-dangle": ["error", "always-multiline"],
      "semi": ["error", "never"],
      "no-console": "error"
    }
  }