{
  "name": "ghog-day",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "DEBUG=ghog-day:* nodemon -e js,njk ./bin/www & npm run scss:watch",
    "lint": "eslint \"./**/*.js\"",
    "scss:watch": "sass --watch src/scss/main.scss public/stylesheets/main.css",
    "scss": "sass src/scss/main.scss public/stylesheets/main.css",
    "start": "npm run scss & node ./bin/www",
    "test": "jest --testPathIgnorePatterns='skip*'"
  },
  "dependencies": {
    "better-sqlite3-helper": "^3.1.6",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "fast-csv": "^4.3.6",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "nunjucks": "^3.2.3",
    "sass": "^1.49.9"
  },
  "devDependencies": {
    "eslint": "^8.11.0",
    "eslint-plugin-jest": "^26.1.1",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.2"
  }
}
